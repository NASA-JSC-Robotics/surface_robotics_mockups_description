<?xml version="1.0"?>
<robot xmlns:xacro="http://wiki.ros.org/xacro">

  <xacro:macro name="trainer_front_right_face" params="
    name:=''
    prefix:=''
    parent:=''
    *origin">
  
  <joint name="${prefix}trainer_front_right_face_joint" type="fixed">
    <parent link="${parent}"/>
    <child link="${prefix}front_right_face"/>
    <xacro:insert_block name="origin"/>
  </joint>

  <link name="${prefix}front_right_face">
    <!--TODO set proper inertia-->
    <inertial>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <mass value="0.0"/>
      <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
    </inertial>
    <visual>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://trainer/meshes/visual/front_right_face.stl"/>
      </geometry>
      <material name="gray"/>
    </visual>
    <collision>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://trainer/meshes/collision/front_right_face.stl"/>
      </geometry>
      <material name="red"/>
    </collision>
  </link>

  <joint name="${prefix}cabinet_1_joint" type="revolute">
    <axis xyz="0 0 1"/>
    <limit effort="10.0" lower="0.0" upper="3.14" velocity="0.5"/>
    <parent link="${prefix}front_right_face"/>
    <child link="${prefix}cabinet_1"/>
    <origin xyz="0.017 ${0.344 + 0.0018} 0.713" rpy="0 0 0"/>
  </joint>

  <link name="${prefix}cabinet_1">
    <!--TODO set proper inertia-->
    <inertial>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <mass value="0.0"/>
      <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
    </inertial>
    <visual>
      <origin xyz="${-0.0075 - 0.003175} -0.00366 0.0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://trainer/meshes/visual/cabinet_door_1.stl"/>
      </geometry>
      <material name="blue"/>
    </visual>
    <collision>
      <origin xyz="${-0.0075 - 0.003175} -0.00366 0.0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://trainer/meshes/collision/cabinet_door_1.stl"/>
      </geometry>
      <material name="red"/>
    </collision>
  </link>

  <joint name="${prefix}round_latch_housing_joint" type="fixed">
    <parent link="${prefix}cabinet_1"/>
    <child link="${prefix}round_latch_housing"/>
    <origin xyz="-0.00547 -0.28855 0.16622" rpy="${pi} 0 0"/>
  </joint>

  <link name="${prefix}round_latch_housing">
    <!--TODO set proper inertia-->
    <inertial>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <mass value="0.0"/>
      <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
    </inertial>
    <visual>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://trainer/meshes/visual/round_latch_housing.stl"/>
      </geometry>
      <material name="gray"/>
    </visual>
    <collision>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://trainer/meshes/collision/round_latch_housing.stl"/>
      </geometry>
      <material name="red"/>
    </collision>
  </link>

  <joint name="${prefix}round_latch_joint" type="revolute">
    <axis xyz="0 0 -1"/>
    <limit effort="10.0" lower="0.0" upper="3.14" velocity="0.5"/>
    <parent link="${prefix}round_latch_housing"/>
    <child link="${prefix}round_latch_handle"/>
    <origin xyz="0.0 -0.0212 -0.0035" rpy="0 0 0"/>
  </joint>

  <link name="${prefix}round_latch_handle">
    <!--TODO set proper inertia-->
    <inertial>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <mass value="0.0"/>
      <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
    </inertial>
    <visual>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://trainer/meshes/visual/round_latch_handle.stl"/>
      </geometry>
      <material name="blue"/>
    </visual>
    <collision>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://trainer/meshes/collision/round_latch_handle.stl"/>
      </geometry>
      <material name="red"/>
    </collision>
  </link>

  <joint name="${prefix}cabinet_2_joint" type="revolute">
    <axis xyz="0 0 -1"/>
    <limit effort="10.0" lower="0.0" upper="3.14" velocity="0.5"/>
    <parent link="${prefix}front_right_face"/>
    <child link="${prefix}cabinet_2"/>
    <origin xyz="0.017 -0.0018 0.379" rpy="0 0 0"/>
  </joint>

  <link name="${prefix}cabinet_2">
    <!--TODO set proper inertia-->
    <inertial>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <mass value="0.0"/>
      <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
    </inertial>
    <visual>
      <origin xyz="${-0.0075 - 0.003175} 0.00366 0.0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://trainer/meshes/visual/cabinet_door_2.stl"/>
      </geometry>
      <material name="blue"/>
    </visual>
    <collision>
      <origin xyz="${-0.0075 - 0.003175} 0.00366 0.0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://trainer/meshes/collision/cabinet_door_2.stl"/>
      </geometry>
      <material name="red"/>
    </collision>
  </link>

  <joint name="${prefix}push_latch_housing_joint" type="fixed">
    <parent link="${prefix}cabinet_2"/>
    <child link="${prefix}push_latch_housing"/>
    <origin xyz="-0.0075 0.2185 0.134" rpy="0 0 0"/>
  </joint>

  <link name="${prefix}push_latch_housing">
    <!--TODO set proper inertia-->
    <inertial>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <mass value="0.0"/>
      <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
    </inertial>
    <visual>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://trainer/meshes/visual/push_latch_housing.stl"/>
      </geometry>
      <material name="gray"/>
    </visual>
    <collision>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://trainer/meshes/collision/push_latch_housing.stl"/>
      </geometry>
      <material name="red"/>
    </collision>
  </link>


  <joint name="${prefix}push_latch_button_joint" type="prismatic">
    <axis xyz="-1 0 0"/>
    <!--TODO button travel is a complete guess at this point-->
    <limit effort="10.0" lower="0.0" upper="0.005" velocity="0.5"/>
    <parent link="${prefix}push_latch_housing"/>
    <child link="${prefix}push_latch_button"/>
    <origin xyz="-0.0125 0.0285 0.0045" rpy="0 0 ${pi/8}"/>
  </joint>

  <link name="${prefix}push_latch_button">
    <!--TODO set proper inertia-->
    <inertial>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <mass value="0.0"/>
      <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
    </inertial>
    <visual>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://trainer/meshes/visual/push_latch_button.stl"/>
      </geometry>
      <material name="blue"/>
    </visual>
    <collision>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://trainer/meshes/collision/push_latch_button.stl"/>
      </geometry>
      <material name="red"/>
    </collision>
  </link>

  <joint name="${prefix}push_latch_handle_joint" type="revolute">
    <axis xyz="0 0 1"/>
    <limit effort="10.0" lower="0.0" upper="${pi/3}" velocity="0.5"/>
    <parent link="${prefix}push_latch_housing"/>
    <child link="${prefix}push_latch_handle"/>
    <origin xyz="-0.003 0.079 0.0045" rpy="0 0 0"/>
  </joint>

  <link name="${prefix}push_latch_handle">
    <!--TODO set proper inertia-->
    <inertial>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <mass value="0.0"/>
      <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
    </inertial>
    <visual>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://trainer/meshes/visual/push_latch_handle.stl"/>
      </geometry>
      <material name="blue"/>
    </visual>
    <collision>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://trainer/meshes/collision/push_latch_handle.stl"/>
      </geometry>
      <material name="red"/>
    </collision>
  </link>


  <joint name="${prefix}cabinet_3_joint" type="revolute">
    <axis xyz="0 0 1"/>
    <limit effort="10.0" lower="0.0" upper="${pi}" velocity="0.5"/>
    <parent link="${prefix}front_right_face"/>
    <child link="${prefix}cabinet_3"/>
    <origin xyz="0.017 ${0.344 + 0.0018} 0.007" rpy="0 0 0"/>
  </joint>

  <link name="${prefix}cabinet_3">
    <!--TODO set proper inertia-->
    <inertial>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <mass value="0.0"/>
      <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
    </inertial>
    <visual>
      <origin xyz="${-0.0075 - 0.003175} -0.00366 0.0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://trainer/meshes/visual/cabinet_door_3.stl"/>
      </geometry>
      <material name="blue"/>
    </visual>
    <collision>
      <origin xyz="${-0.0075 - 0.003175} -0.00366 0.0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://trainer/meshes/collision/cabinet_door_3.stl"/>
      </geometry>
      <material name="red"/>
    </collision>
  </link>

  </xacro:macro>
</robot>
