<?xml version="1.0"?>
<robot xmlns:xacro="http://wiki.ros.org/xacro" name="4040_hatch">
  <xacro:arg name="name" default=""/>
  <xacro:arg name="prefix" default=""/>

  <xacro:include filename="$(find 4040_hatch)/urdf/hatch_face.xacro"/>

  <link name="world"/>

  <xacro:hatch_face name="$(arg name)" prefix="$(arg prefix)" parent="world">
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </xacro:hatch_face>

</robot>
